{
    "name": "maduser/argon-routing",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Maduser\\Argon\\Routing\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Julien Duseyau",
            "email": "julien.duseyau@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "~/workspace/argon/packages/maduser-argon"
        },
        {
            "type": "path",
            "url": "~/workspace/argon/packages/maduser-argon-middleware"
        }
    ],
    "require": {
        "psr/http-factory": "^1.1",
        "psr/http-message": "^2.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^3.0",
        "maduser/argon-middleware": "dev-master"
    },
    "require-dev": {
        "maduser/argon": "dev-master",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "phpunit/phpunit": "^9.6",
        "slevomat/coding-standard": "^8.16",
        "squizlabs/php_codesniffer": "^3.8",
        "symfony/var-dumper": "^7.2",
        "vimeo/psalm": "^5.26",
        "vlucas/phpdotenv": "^5.6"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "platform": {
            "php": "8.2"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
